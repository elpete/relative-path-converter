{
    "name": "Relative Path Converter",
    "version": "1.0.0",
    "author": "Eric Peterson",
    "createPackageDirectory": true,
    "location": "https://github.com/elpete/relative-path-converter/archive/master.zip",
    "homepage": "https://github.com/elpete/relative-path-converter",
    "documentation": "https://github.com/elpete/relative-path-converter",
    "repository": {
        "type": "git",
        "URL": "https://github.com/elpete/relative-path-converter"
    },
    "bugs": "https://github.com/elpete/relative-path-converter/issues",
    "slug": "relative-path-converter",
    "shortDescription": "Translate an absolute path into a relative path.",
    "description": "Translate an absolute path into a relative path given a context directory",
    "instructions": "README.md",
    "changelog": "https://github.com/elpete/relative-path-converter",
    "type": "modules",
    "keywords": [
        "path",
        "filesystem",
        "relative",
        "absolute"
    ],
    "private": false,
    "engines": [
        { "type": "lucee", "version": ">=4.5.x" },
        { "type": "railo", "version": ">=4.1.x" },
        { "type": "adobe", "version": ">=10.0.0" }
    ],
    "defaultEngine": "lucee",
    "defaultPort": 8080,
    "license": [
        { "type": "MIT", "URL": "https://github.com/elpete/relative-path-converter/blob/master/LICENSE" }
    ],
    "dependencies": {},
    "devDependencies":{
        "testbox": "2.0.0"
    },
    "installPaths": {
        "testbox":"testbox"
    },
    "ignore": [
        "**/.*",
        "test",
        "tests"
    ]
}